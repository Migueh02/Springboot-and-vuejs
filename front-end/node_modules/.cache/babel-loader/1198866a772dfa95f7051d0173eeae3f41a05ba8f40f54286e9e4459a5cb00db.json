{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      estudiante: {\n        id: null,\n        cc: \"\",\n        Nombre: \"\",\n        Apellido: \"\",\n        Telefono: \"\",\n        Email: \"\",\n        Carrera: \"\"\n      }\n    };\n  },\n  methods: {\n    async getEstudiante(id) {\n      try {\n        const response = await axios.get(`http://localhost:8080/api/uniminuto/id/${id}`);\n        this.estudiante = response.data;\n      } catch (error) {\n        console.error(\"Error al obtener el estudiante:\", error);\n      }\n    },\n    async updateEstudiante() {\n      try {\n        const response = await axios.put(`http://localhost:8080/api/uniminuto/estudiantes/${this.estudiante.id}`, this.estudiante);\n        alert(\"Estudiante actualizado correctamente\");\n        this.$router.push(`/edit-estudiante/${this.estudiante.id}`); // Redirigir a la lista de estudiantes\n      } catch (error) {\n        console.error(\"Error al actualizar el estudiante:\", error);\n      }\n    }\n  },\n  mounted() {\n    const estudianteId = this.$route.params.id; // Obtener el id del estudiante desde la URL\n    this.getEstudiante(estudianteId);\n  }\n};","map":{"version":3,"names":["axios","data","estudiante","id","cc","Nombre","Apellido","Telefono","Email","Carrera","methods","getEstudiante","response","get","error","console","updateEstudiante","put","alert","$router","push","mounted","estudianteId","$route","params"],"sources":["/home/migueh/Front Vue/estudiante-crud/src/components/EstudiantesEditar.vue"],"sourcesContent":["<template>\n  <div>\n    <h2>Editar Estudiante</h2>\n    <form @submit.prevent=\"updateEstudiante\">\n      <div>\n        <label for=\"cc\">Cédula:</label>\n        <input v-model=\"estudiante.cc\" type=\"number\" id=\"cc\" required />\n      </div>\n      <div>\n        <label for=\"Nombre\">Nombre:</label>\n        <input v-model=\"estudiante.Nombre\" type=\"text\" id=\"Nombre\" required />\n      </div>\n      <div>\n        <label for=\"Apellido\">Apellido:</label>\n        <input v-model=\"estudiante.Apellido\" type=\"text\" id=\"Apellido\" required />\n      </div>\n      <div>\n        <label for=\"Telefono\">Teléfono:</label>\n        <input v-model=\"estudiante.Telefono\" type=\"text\" id=\"Telefono\" required />\n      </div>\n      <div>\n        <label for=\"Email\">Correo:</label>\n        <input v-model=\"estudiante.Email\" type=\"email\" id=\"Email\" required />\n      </div>\n      <div>\n        <label for=\"Carrera\">Carrera:</label>\n        <input v-model=\"estudiante.Carrera\" type=\"text\" id=\"Carrera\" required />\n      </div>\n      <button type=\"submit\">Actualizar Estudiante</button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  data() {\n    return {\n      estudiante: {\n        id: null,\n        cc: \"\",\n        Nombre: \"\",\n        Apellido: \"\",\n        Telefono: \"\",\n        Email: \"\",\n        Carrera: \"\",\n      },\n    };\n  },\n  methods: {\n    async getEstudiante(id) {\n      try {\n        const response = await axios.get(`http://localhost:8080/api/uniminuto/id/${id}`);\n        this.estudiante = response.data;\n      } catch (error) {\n        console.error(\"Error al obtener el estudiante:\", error);\n      }\n    },\n    async updateEstudiante() {\n      try {\n        const response = await axios.put(`http://localhost:8080/api/uniminuto/estudiantes/${this.estudiante.id}`, this.estudiante);\n        alert(\"Estudiante actualizado correctamente\");\n        this.$router.push(`/edit-estudiante/${this.estudiante.id}`); // Redirigir a la lista de estudiantes\n      } catch (error) {\n        console.error(\"Error al actualizar el estudiante:\", error);\n      }\n    },\n  },\n  mounted() {\n    const estudianteId = this.$route.params.id; // Obtener el id del estudiante desde la URL\n    this.getEstudiante(estudianteId);\n  },\n};\n</script>\n"],"mappings":";AAkCA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE;QACVC,EAAE,EAAE,IAAI;QACRC,EAAE,EAAE,EAAE;QACNC,MAAM,EAAE,EAAE;QACVC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,OAAO,EAAE;MACX;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,aAAaA,CAACR,EAAE,EAAE;MACtB,IAAI;QACF,MAAMS,QAAO,GAAI,MAAMZ,KAAK,CAACa,GAAG,CAAC,0CAA0CV,EAAE,EAAE,CAAC;QAChF,IAAI,CAACD,UAAS,GAAIU,QAAQ,CAACX,IAAI;MACjC,EAAE,OAAOa,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,iCAAiC,EAAEA,KAAK,CAAC;MACzD;IACF,CAAC;IACD,MAAME,gBAAgBA,CAAA,EAAG;MACvB,IAAI;QACF,MAAMJ,QAAO,GAAI,MAAMZ,KAAK,CAACiB,GAAG,CAAC,mDAAmD,IAAI,CAACf,UAAU,CAACC,EAAE,EAAE,EAAE,IAAI,CAACD,UAAU,CAAC;QAC1HgB,KAAK,CAAC,sCAAsC,CAAC;QAC7C,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,oBAAoB,IAAI,CAAClB,UAAU,CAACC,EAAE,EAAE,CAAC,EAAE;MAC/D,EAAE,OAAOW,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,oCAAoC,EAAEA,KAAK,CAAC;MAC5D;IACF;EACF,CAAC;EACDO,OAAOA,CAAA,EAAG;IACR,MAAMC,YAAW,GAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACrB,EAAE,EAAE;IAC5C,IAAI,CAACQ,aAAa,CAACW,YAAY,CAAC;EAClC;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}